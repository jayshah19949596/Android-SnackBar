<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jaysh.snackbar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LogIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUPActivity"
            android:parentActivityName=".LogIn" />
        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CartScreen"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".Payment"
            android:parentActivityName=".CartScreen" />
        <activity android:name=".AdminHome" />
        <activity
            android:name=".EmployeeTasks"
            android:parentActivityName=".AdminHome" />
        <activity
            android:name=".DishTasks"
            android:parentActivityName=".AdminHome" />
        <activity android:name=".ChefHome" />
        <activity
            android:name=".AddEmployee"
            android:parentActivityName=".EmployeeTasks" />
        <activity
            android:name=".ModifyEmployee"
            android:parentActivityName=".EmployeeTasks" />
        <activity
            android:name=".OrderScreen"
            android:parentActivityName=".ChefHome" />
        <activity
            android:name=".DeleteEmployee"
            android:parentActivityName=".EmployeeTasks" />

        <activity android:name=".AddDish"
            android:parentActivityName=".DishTasks" />

        <activity android:name=".ModifyDish"
            android:parentActivityName=".DishTasks" />

        <activity android:name=".DeleteDish"
            android:parentActivityName=".DishTasks" />

    </application>

</manifest>